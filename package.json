{
  "name": "react-nerdery-clone-app-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fakeBuild": "ts-node --esm src/util/places.ts && ts-node --esm src/util/user.ts",
    "fakeMake": "npm run fakeBuild && json-server --watch src/API/local/index.cjs --port 3030",
    "fakeStart": "json-server --watch src/API/local/index.cjs --port 3030",
    "fakeAuth": "json-server src/API/local/index.cjs -m ./node_modules/json-server-auth",
    "test": "vitest",
    "testCoverage": "vitest run --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@jackfranklin/test-data-bot": "^2.1.0",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-router-dom": "^6.11.1",
    "short-uuid": "^4.2.2",
    "styled-components": "^5.3.10",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "express": "^4.18.2",
    "gh-pages": "^5.0.0",
    "history": "^5.3.0",
    "jsdom": "^22.0.0",
    "json-server": "^0.17.3",
    "json-server-auth": "^2.1.0",
    "msw": "^1.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.31.0"
  }
}
